Cell[TextData[{
 "It is already natural to write ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    RowBox[{
     RowBox[{"x", "&"}], "y"}], ")"}], TraditionalForm]],ExpressionUUID->
  "7e87ebd0-5632-42c1-ae51-b375a388edf0"],
