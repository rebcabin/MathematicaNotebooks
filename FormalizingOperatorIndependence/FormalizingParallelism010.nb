 FormBox[GridBox[{
    {"Init", 
     OverscriptBox["=", "def"], 
     RowBox[{"every", " ", "value", " ", "in", " ", 
      SubscriptBox["\[Psi]", "0"], " ", "is", " ", "either", " ", "1", " ", 
      "or", " ", "0"}]},
    {
     RowBox[{"Next", "(", 
      RowBox[{"x", ",", "s"}], ")"}], 
