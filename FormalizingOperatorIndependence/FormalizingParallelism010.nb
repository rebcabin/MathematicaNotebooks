Cell[TextData[{
 "A very important restriction on these considerations, ",
 StyleBox["is that ",
  FontWeight->"Bold",
  FontSlant->"Italic"],
 Cell[BoxData[
  FormBox["x", TraditionalForm]],
  FontWeight->"Bold",
  FontSlant->"Italic",ExpressionUUID->
  "dca6aa62-61c8-404e-a0ce-7df23fba1c10"],
