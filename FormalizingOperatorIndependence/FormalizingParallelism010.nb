Cell[TextData[{
 "Machine variables and functions are in all caps, like ",
 StyleBox["RL", "Program"],
 ", ",
 StyleBox["GL", "Program"],
 ", ",
 StyleBox["GL()", "Program"],
 ", ",
 StyleBox["GGL", "Program"],
 ", ",
 StyleBox["BGGL()", "Program"],
 ", ",
 StyleBox["RSP16", "Program"],
 ", ",
 StyleBox["BRSP16()", "Program"],
 ". User-visible BELEX variables and functions, are in lower case, like ",
 StyleBox["add_u16", "Program"],
 ", ",
 Cell[BoxData[
  FormBox["x", TraditionalForm]],ExpressionUUID->
  "b59eb7c1-9319-4e95-9b33-c0e495cef028"],
 ", ",
 Cell[BoxData[
  FormBox["y", TraditionalForm]],ExpressionUUID->
  "8061d606-bf38-4303-b71e-0c0eceaee229"],
 ", and ",
 Cell[BoxData[
  FormBox["x\[UpPointer]y", TraditionalForm]],ExpressionUUID->
  "ec5d13e7-f560-422d-9ed9-4eed8b251f61"],
 ". "
}], "Text",
 CellChangeTimes->{{3.829726307425819*^9, 3.8297263668720293`*^9}, {
