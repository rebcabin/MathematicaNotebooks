Cell[BoxData[{
 RowBox[{
  RowBox[{"ClearAll", "[", 
   RowBox[{"sectionListFromSectionMask", ",", "sectionMaskFromSectionList"}], 
   "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"sectionListFromSectionMask", "[", "mask_", "]"}], ":=", 
   RowBox[{"Flatten", "[", 
    RowBox[{
     RowBox[{"Position", "[", 
      RowBox[{
       RowBox[{"Reverse", "[", 
        RowBox[{"IntegerDigits", "[", 
         RowBox[{"mask", ",", "2"}], "]"}], "]"}], ",", "1"}], "]"}], "-", 
     "1"}], "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"sectionMaskFromSectionList", "[", "bitNumbers_", "]"}], ":=", " ", 
   RowBox[{"Plus", "@@", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{
       SuperscriptBox["2", "#1"], "&"}], "/@", "bitNumbers"}], ")"}]}]}], 
  ";"}]}], "Input",
