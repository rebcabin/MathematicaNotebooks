Cell[TextData[{
 "The XOR of two random bits have probability ",
 Cell[BoxData[
  FormBox[
   RowBox[{"1", "/", "2"}], TraditionalForm]],ExpressionUUID->
  "512ce393-08df-4615-affc-33521763aae7"],
 " of being ON, so the expected number of ON bits in the XOR of ",
 Cell[BoxData[
  FormBox["N", TraditionalForm]],ExpressionUUID->
  "baa119cb-8c7b-4b40-b04e-e214712e1ee3"],
 " random bits is ",
 Cell[BoxData[
  FormBox[
   RowBox[{"N", "/", "2"}], TraditionalForm]],ExpressionUUID->
  "19911058-8937-43c6-bca1-15e3a64788e7"],
 "."
}], "Text",
 CellChangeTimes->{
  3.8290739725024443`*^9, {3.8290741205770855`*^9, 3.8290741553580775`*^9}, {
