Cell[BoxData[
 RowBox[{"TautologyQ", "[", "\[IndentingNewLine]", 
  RowBox[{
   RowBox[{
    RowBox[{"a", "\[Or]", 
     RowBox[{"(", 
      RowBox[{"b", "\[And]", "c"}], ")"}]}], "\[Equivalent]", 
    "\[IndentingNewLine]", 
    RowBox[{
