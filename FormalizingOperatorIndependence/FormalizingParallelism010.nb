Cell["\<\
@basic_block(rn_actuals=['res', 'x', 'y', 'x_xor_y', 'cout1', 'flags'],
             sm_actuals={'os': \"0001\", 'fs': 'FFFF', 'threes': '3333',
                         'ones': '1111', 'one_f': '000F'},
             unique_frag_nym='snip_add_u16_gvml_bb')
def bb_add_u16_24_gvml(p: BLECCI, symbols_to_rp_nyms: Dict[str, str]):
    res_rp, x_rp, y_rp, x_or_y_rp, cout1_rp, flags_rp, \\
    os_rp, fs_rp, threes_rp, ones_rp, one_f_rp = \\
        symbols_to_rp_nyms.values()

    # from libs-gvml/src/include/arith_cmds_inst.apl.h:#define \
ARITH_ADD_U16_GL_CO_FLAG_CO_T0_INST(x, y, res, x_xor_y, cout1)

    # ./gsi/apl_defs.h:66:      SM_BIT_0 = 0,
    # ./gsi/gvml_apl.h:60:      C_FLAG          = SM_BIT_0,             /* \
Carry in/o

