  3.8305918789729567`*^9}, {3.830591912405446*^9, 3.830591936178023*^9}, {
  3.8305939774785166`*^9, 3.8305940177234306`*^9}, {3.8306268885830703`*^9, 
  3.8306269536108847`*^9}, {3.830627426291074*^9, 3.830627546511921*^9}, {
  3.8307800318343177`*^9, 3.8307802723316584`*^9}, {3.8307809184087014`*^9, 
