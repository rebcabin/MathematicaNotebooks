 FormBox[
  RowBox[{
   RowBox[{"\[LeftGuillemet]", " ", 
    RowBox[{"MMB", "[", 
     RowBox[{
      SubscriptBox["v", "MMB"], ",", 
      SubscriptBox["p", "MMB"], ",", 
      SubscriptBox["s", "MMB"]}], "]"}]}], ",", " ", 
   RowBox[{"RL", "[", 
    RowBox[{
