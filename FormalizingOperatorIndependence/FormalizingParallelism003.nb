Cell[TextData[{
 "A ",
 StyleBox["step",
  FontWeight->"Bold",
  FontSlant->"Italic"],
 " is a contiguous pair of states in a behavior, ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    RowBox[{
     SubscriptBox["\[Psi]", "i"], ",", 
     SubscriptBox["\[Psi]", 
      RowBox[{"i", "+", "1"}]]}], ")"}], TraditionalForm]],ExpressionUUID->
  "b4e65af1-76fd-44e9-a258-a6e024364350"],
