\t\t(SM_0X1111<<2):\t\tRL |= SB[x_xor_y] & NRL;\t/* (CIN & xXORy) */\t\\
\t\tSM_0X0001:\t\tGGL = RL;\t\t\t\t\t\t\\
\t}{ /* 7 */\t\t\t\t\t\t\t\t\t\t\\
\t\t(SM_0X1111<<3):\t\tRL |= SB[x_xor_y] & NRL;\t/* (CIN & xXORy) */\t\\
\t\t(SM_0X0001<<3):\t\tGL = RL;\t\t\t\t\t\t\\
\t\tSM_0X0001:\t\tRL = SB[cout1];\t\t\t\t\t\t\\
\t}{ /* 8 */\t\t\t\t\t\t\t\t\t\t\\
\t\t(SM_0X000F<<4):\t\tRL |= SB[cout1] & GL;\t\t\t\t\t\\
\t\t(SM_0X0001<<7):\t\tGL = RL;\t\t\t\t\t\t\\
\t\tSM_0X0001:\t\tSB[res] = RL;\t\t\t\t\t\t\\
\t}{ /* 9 */\t\t\t\t\t\t\t\t\t\t\\
\t\t(SM_0X000F<<8):\t\tRL |= SB[cout1] & GL;\t\t\t\t\t\\
\t\t(SM_0X0001<<11):\tGL = RL;\t\t\t\t\t\t\\
\t\tSM_0X0001:\t\tRL = GGL;\t\t\t\t\t\t\\
\t}{ /* 10 */\t\t\t\t\t\t\t\t\t\t\\
\t\t(SM_0X000F<<12):\tRL |= SB[cout1] & GL;\t\t\t\t\t\\
