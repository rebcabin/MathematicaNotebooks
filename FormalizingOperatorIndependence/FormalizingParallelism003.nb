Cell[TextData[{
 "In general, even in our huge, notional, ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    SuperscriptBox["2", "831488"], "\[Times]", "831488"}], TraditionalForm]],
  ExpressionUUID->"c3531344-01bb-49a3-ba65-aaa9361586e0"],
