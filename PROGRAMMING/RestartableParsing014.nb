Cell[TextData[{
 "This scanner first replays all validated characters in ",
 StyleBox["scannerState",
  FontSlant->"Italic"],
 " to handle restart or re-entry. Any action but ",
 StyleBox["save",
  FontSlant->"Italic"],
 " during replay is a catastrophic error (",
 StyleBox["Throw",
  FontSlant->"Italic"],
 ", useful only in simulation; some other method of handling catastrophic \
errors must be implemented in a real, embedded application). "
}], "Text",
 CellChangeTimes->{{3.918646611985779*^9, 3.918646761392509*^9}, {
  3.918647184799543*^9, 3.918647189518314*^9}, {3.918660310051354*^9, 
  3.918660319583208*^9}},ExpressionUUID->"90e17b02-701f-4a61-bfea-\
0d256f2c9889"],

Cell[TextData[{
 StyleBox["Scanner",
  FontSlant->"Italic"],
 " performs actions before optimistically reading characters from ",
 StyleBox["inputState",
  FontSlant->"Italic"],
 " and transitioning to the next vertex. When ",
 StyleBox["inputState",
  FontSlant->"Italic"],
 " is empty, the scanner stops. "
}], "Text",
 CellChangeTimes->{{3.918646879921524*^9, 3.9186469700315437`*^9}, {
   3.9186471939017973`*^9, 3.9186471939800243`*^9}, {3.9186473049005337`*^9, 
   3.918647340124117*^9}, 3.9186603297255907`*^9, {3.918660370419675*^9, 
   3.918660408628131*^9}},ExpressionUUID->"ea749fdf-325d-4a91-b081-\
897b0db10d24"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"ClearAll", "[", "scanner", "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"scanner", "[", "\[IndentingNewLine]", 
    RowBox[{
    "scannerStateParam_", ",", "\[IndentingNewLine]", 
     "provisionalPayloadParam_", ",", "\[IndentingNewLine]", 
     "inputStateParam_", ",", "\[IndentingNewLine]", "fsmStepFunction_"}], 
    "]"}], ":=", "\[IndentingNewLine]", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"(*", 
       RowBox[{"local", " ", "variables"}], "*)"}], "\[IndentingNewLine]", 
      RowBox[{"(*", 
       RowBox[{"shallow", " ", "copies"}], "*)"}], "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"ss", "=", "scannerStateParam"}], ",", "\[IndentingNewLine]", 
       RowBox[{"ps", "=", "provisionalPayloadParam"}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"is", "=", "inputStateParam"}], ",", "\[IndentingNewLine]", 
       RowBox[{"(*", 
        RowBox[{"for", " ", "output"}], "*)"}], "\[IndentingNewLine]", 
       RowBox[{"metrics", "=", 
        RowBox[{"<|", 
         RowBox[{
          RowBox[{"\"\<discarded\>\"", "\[Rule]", "0"}], ",", 
          RowBox[{"\"\<saved\>\"", "\[Rule]", "0"}]}], "|>"}]}], ",", 
       "\[IndentingNewLine]", 
       RowBox[{"matches", "=", 
        RowBox[{"<|", "|>"}]}], ",", "\[IndentingNewLine]", 
       RowBox[{"(*", 
        RowBox[{"for", " ", "stepping", " ", "the", " ", "FSM"}], "*)"}], 
       "\[IndentingNewLine]", "oldHead", ",", 
       RowBox[{"(*", 
        RowBox[{
         RowBox[{"Save", " ", "original", " ", "scanner"}], "-", 
         RowBox[{
         "state", " ", "head", " ", "because", " ", "pops", " ", "of", " ", 
          "valid", " ", "characters", " ", "may", " ", 
          RowBox[{"occur", "."}]}]}], "*)"}], "\[IndentingNewLine]", "vertex",
        ",", "\[IndentingNewLine]", "character", ",", "\[IndentingNewLine]", 
       "action", ",", 
       RowBox[{"(*", 
        RowBox[{"ignored", " ", "at", " ", "first"}], "*)"}], 
       "\[IndentingNewLine]", "actionArgument"}], 
      RowBox[{"(*", 
       RowBox[{"ignored", " ", "at", " ", "first"}], "*)"}], "}"}], ",", 
     "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{
      "Advance", " ", "the", " ", "FSM", " ", "from", " ", "Entry", " ", "to",
        " ", "its", " ", "first", " ", "actionable", " ", 
       RowBox[{"state", "."}]}], " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{
       RowBox[{"{", 
        RowBox[{
        "vertex", ",", "action", ",", "actionArgument", ",", "matches"}], 
        "}"}], "=", "\[IndentingNewLine]", 
       RowBox[{"fsmStepFunction", "[", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{"\"\<Entry\>\"", ",", "Null", ",", "Null", ",", "matches"}],
           "}"}], ",", "Null"}], "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{
       "Step", " ", "the", " ", "FSM", " ", "with", " ", "\.7fvalidated", " ",
         "characters", " ", "from", " ", "scanner", " ", 
        RowBox[{"state", "."}]}], " ", "*)"}], "\[IndentingNewLine]", 
      RowBox[{"oldHead", "=", 
       RowBox[{"ss", "[", "\"\<head\>\"", "]"}]}], ";", "\[IndentingNewLine]", 
      RowBox[{"While", "[", 
       RowBox[{
        RowBox[{"Not", "[", 
         RowBox[{"CBemptyQ", "[", "ss", "]"}], "]"}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"(*", " ", 
         RowBox[{
         "No", " ", "need", " ", "to", " ", "check", " ", "for", " ", "Exit", 
          " ", "and", " ", "Fault", " ", "because", " ", "of", " ", "the", 
          " ", "invariant", " ", "that", " ", "the", " ", "scanner", " ", 
          "state", " ", "contains", " ", "only", " ", "valid", " ", 
          RowBox[{"characters", "."}]}], " ", "*)"}], "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{
          RowBox[{"{", 
           RowBox[{"ss", ",", "character"}], "}"}], "=", 
          RowBox[{"CBpop", "[", "ss", "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{
           "vertex", ",", "action", ",", "actionArgument", ",", "matches"}], 
           "}"}], "=", "\[IndentingNewLine]", 
          RowBox[{"fsmStepFunction", "[", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{
             "vertex", ",", "action", ",", "actionArgument", ",", "matches"}],
              "}"}], ",", "character"}], "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"Switch", "[", 
          RowBox[{
          "action", ",", "\[IndentingNewLine]", "\"\<save\>\"", ",", "Null", 
           ",", 
           RowBox[{"(*", 
            RowBox[{
             RowBox[{"do", " ", "nothing"}], ";", " ", 
             RowBox[{
             "characters", " ", "in", " ", "scanner", " ", "state", " ", 
              "have", " ", "already", " ", "been", " ", "validated"}]}], 
            "*)"}], "\[IndentingNewLine]", "\"\<clear\>\"", ",", 
           RowBox[{"Throw", "[", 
            RowBox[{"\"\<1: Invalid character \>\"", "<>", 
             RowBox[{"ToString", "@", "character"}], "<>", 
             "\"\< in scanner state\>\""}], "]"}], ",", 
           RowBox[{"(*", 
            RowBox[{"should", " ", "never", " ", "happen"}], "*)"}], 
           "\[IndentingNewLine]", "\"\<discard\>\"", ",", 
           RowBox[{"Throw", "[", 
            RowBox[{"\"\<2: Invalid character \>\"", "<>", 
             RowBox[{"ToString", "@", "character"}], "<>", 
             "\"\< in scanner state\>\""}], "]"}], ",", 
           RowBox[{"(*", 
            RowBox[{"should", " ", "never", " ", "happen"}], "*)"}], 
           "\[IndentingNewLine]", "\"\<no action\>\"", ",", 
           RowBox[{"Throw", "[", 
            RowBox[{"\"\<5: Invalid character \>\"", "<>", 
             RowBox[{"ToString", "@", "character"}], "<>", 
             "\"\< in scanner state\>\""}], "]"}], ",", 
           RowBox[{"(*", 
            RowBox[{"should", " ", "never", " ", "happen"}], "*)"}], 
           "\[IndentingNewLine]", "default_", ",", 
           RowBox[{"Throw", "[", 
            RowBox[{"\"\<3: Invalid action \>\"", "<>", 
             RowBox[{"ToString", "@", "action"}], "<>", 
             "\"\< returned by fsm step on character \>\"", "<>", 
             RowBox[{"ToString", "@", "character"}]}], "]"}]}], 
          RowBox[{"(*", 
           RowBox[{"should", " ", "never", " ", "happen"}], "*)"}], 
          "\[IndentingNewLine]", "]"}]}]}], 
       RowBox[{"(*", 
        RowBox[{"switch", " ", "action"}], "*)"}], "\[IndentingNewLine]", 
       "]"}], ";", 
      RowBox[{"(*", 
       RowBox[{
       "while", " ", "scanning", " ", "prior", " ", "scanner", " ", "state"}],
        "*)"}], "\[IndentingNewLine]", 
      RowBox[{"(*", " ", 
       RowBox[{
       "Reset", " ", "head", " ", "of", " ", "scanner", " ", "state", " ", 
        "to", " ", "old", " ", "head", " ", "because", " ", "pops", " ", "of",
         " ", "valid", " ", "characters", " ", "may", " ", "have", " ", 
        RowBox[{"occurred", "."}]}], " ", "*)"}], "\[IndentingNewLine]", 
      RowBox[{"ss", "=", 
       RowBox[{"CBreset", "[", 
        RowBox[{"ss", ",", "oldHead", ",", 
         RowBox[{"ss", "[", "\"\<tail\>\"", "]"}]}], "]"}]}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"While", "[", 
       RowBox[{
        RowBox[{"Not", "[", 
         RowBox[{"CBemptyQ", "[", "is", "]"}], "]"}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{"(*", 
         RowBox[{
         "Take", " ", "characters", " ", "from", " ", "input", " ", "state"}],
          "*)"}], "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{
          RowBox[{"{", 
           RowBox[{"is", ",", "character"}], "}"}], "=", 
          RowBox[{"CBpop", "[", "is", "]"}]}], ";", "\[IndentingNewLine]", 
         "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{
           "vertex", ",", "action", ",", "actionArgument", ",", "matches"}], 
           "}"}], "=", "\[IndentingNewLine]", 
          RowBox[{"fsmStepFunction", "[", "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{
             "vertex", ",", "action", ",", "actionArgument", ",", "matches"}],
              "}"}], ",", "\[IndentingNewLine]", "character"}], "]"}]}], ";", 
         "\[IndentingNewLine]", "\[IndentingNewLine]", 
         RowBox[{"Switch", "[", 
          RowBox[{
          "action", ",", "\[IndentingNewLine]", "\[IndentingNewLine]", 
           "\"\<save\>\"", ",", "\[IndentingNewLine]", 
           RowBox[{"Switch", "[", 
            RowBox[{
            "actionArgument", ",", "\[IndentingNewLine]", "\"\<validated\>\"",
              ",", 
             RowBox[{"(", 
              RowBox[{
               RowBox[{
                RowBox[{"metrics", "[", "\"\<saved\>\"", "]"}], "++"}], ";", 
               RowBox[{"ss", "=", 
                RowBox[{"CBpush", "[", 
                 RowBox[{"ss", ",", "character"}], "]"}]}], ";"}], ")"}], ",",
              "\[IndentingNewLine]", "\"\<provisional\>\"", ",", 
             RowBox[{"(", 
              RowBox[{"ps", "=", 
               RowBox[{"CBpush", "[", 
                RowBox[{"ps", ",", "character"}], "]"}]}], ")"}], ",", 
             "\[IndentingNewLine]", "default_", ",", 
             RowBox[{"Throw", "[", 
              RowBox[{"\"\<6: Invalid action argument \>\"", "<>", 
               RowBox[{"ToString", "[", "actionArgument", "]"}]}], "]"}]}], 
            "]"}], ",", "\[IndentingNewLine]", "\[IndentingNewLine]", 
           "\"\<clear\>\"", ",", "\[IndentingNewLine]", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{
              RowBox[{"metrics", "[", "\"\<discarded\>\"", "]"}], "+=", 
              RowBox[{"CBlen", "[", "ss", "]"}]}], ";", 
             RowBox[{
              RowBox[{"metrics", "[", "\"\<saved\>\"", "]"}], "=", "0"}], ";", 
             RowBox[{"ss", "=", 
              RowBox[{"CBclear", "[", "ss", "]"}]}]}], ")"}], ",", 
           "\[IndentingNewLine]", "\[IndentingNewLine]", "\"\<discard\>\"", 
           ",", "\[IndentingNewLine]", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"metrics", "[", "\"\<discarded\>\"", "]"}], "++"}], 
            ")"}], ",", "\[IndentingNewLine]", "\[IndentingNewLine]", 
           "\"\<washback\>\"", ",", "\[IndentingNewLine]", 
           RowBox[{"With", "[", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{"count", "=", "actionArgument"}], "}"}], ",", 
             "\[IndentingNewLine]", 
             RowBox[{
              RowBox[{"If", "[", 
               RowBox[{
                RowBox[{
                 RowBox[{"Not", "[", 
                  RowBox[{"NumberQ", "[", "count", "]"}], "]"}], "||", 
                 RowBox[{"count", "<", "0"}]}], ",", "\[IndentingNewLine]", 
                RowBox[{"Throw", "[", 
                 RowBox[{"\"\<6: invalid washback count \>\"", "<>", 
                  RowBox[{"ToString", "[", "count", "]"}]}], "]"}]}], "]"}], 
              ";", "\[IndentingNewLine]", 
              RowBox[{
               RowBox[{"{", 
                RowBox[{"is", ",", "ps"}], "}"}], "=", 
               RowBox[{"washBack", "[", 
                RowBox[{"is", ",", "ps", ",", "count"}], "]"}]}]}]}], "]"}], 
           ",", "\[IndentingNewLine]", "\[IndentingNewLine]", 
           "\"\<no action\>\"", ",", "\[IndentingNewLine]", "Null", ",", 
           "\[IndentingNewLine]", "\[IndentingNewLine]", "default_", ",", 
           RowBox[{"(*", 
            RowBox[{"pattern", " ", "that", " ", "matches", " ", "anything"}],
             "*)"}], "\[IndentingNewLine]", 
           RowBox[{"Throw", "[", 
            RowBox[{"\"\<4: Invalid action \>\"", "<>", 
             RowBox[{"ToString", "@", "action"}], "<>", 
             "\"\< returned by fsm step on character \>\"", "<>", 
             RowBox[{"ToString", "@", "character"}]}], "]"}]}], 
          "\[IndentingNewLine]", "\[IndentingNewLine]", "]"}], ";", 
         RowBox[{"(*", 
          RowBox[{"switch", " ", "action"}], "*)"}], "\[IndentingNewLine]", 
         "\[IndentingNewLine]", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"vertex", "===", "\"\<Exit\>\""}], ",", 
           "\[IndentingNewLine]", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{
              RowBox[{"{", 
               RowBox[{"ss", ",", "ps"}], "}"}], "=", 
              RowBox[{"transferAll", "[", 
               RowBox[{"ss", ",", "ps"}], "]"}]}], ")"}], ";", 
            RowBox[{"Break", "[", "]"}]}]}], "]"}], ";", 
         "\[IndentingNewLine]", "\[IndentingNewLine]", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"vertex", "===", "\"\<Fault\>\""}], ",", 
           "\[IndentingNewLine]", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"ss", "=", 
              RowBox[{"CBclear", "[", "ss", "]"}]}], ";", 
             "\[IndentingNewLine]", 
             RowBox[{"Break", "[", "]"}]}], ")"}]}], "]"}], ";"}]}], 
       "\[IndentingNewLine]", "\[IndentingNewLine]", "]"}], ";", 
      RowBox[{"(*", 
       RowBox[{
       "while", " ", "scanning", " ", "input", " ", "and", " ", 
        "transferring", " ", "chacters", " ", "to", " ", "scanner", " ", 
        "state"}], "*)"}], "\[IndentingNewLine]", "\[IndentingNewLine]", 
      RowBox[{"(*", "return", "*)"}], 
      RowBox[{"{", 
       RowBox[{"ss", ",", "ps", ",", "is", ",", "metrics", ",", "matches"}], 
       "}"}]}]}], "\[IndentingNewLine]", "]"}]}], ";"}]}], "Input",
 CellChangeTimes->{{3.689266752140547*^9, 3.689266866576077*^9}, {
