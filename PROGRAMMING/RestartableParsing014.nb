Cell[BoxData[{
 RowBox[{
  RowBox[{"ClearAll", "[", "washBack", "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"washBack", "[", 
    RowBox[{"destBuf_", ",", "srcBuf_", ",", "n_"}], "]"}], ":=", 
   "\[IndentingNewLine]", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"c", ",", 
       RowBox[{"i", "=", "n"}], ",", 
       RowBox[{"dest", "=", "destBuf"}], ",", 
       RowBox[{"src", "=", "srcBuf"}]}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"While", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"Not", "[", 
          RowBox[{"CBemptyQ", "[", "src", "]"}], "]"}], "&&", 
         RowBox[{"(", 
          RowBox[{"i", ">", "0"}], ")"}]}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"i", "=", 
          RowBox[{"i", "-", "1"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{"{", 
           RowBox[{"src", ",", "c"}], "}"}], "=", 
          RowBox[{"CBbackspace", "[", "src", "]"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"dest", "=", 
          RowBox[{"CBpushBack", "[", 
           RowBox[{"dest", ",", "c"}], "]"}]}]}]}], "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"{", 
       RowBox[{"dest", ",", "src"}], "}"}]}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"ClearAll", "[", "transferAll", "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"transferAll", "[", 
    RowBox[{"destBuf_", ",", "srcBuf_"}], "]"}], ":=", "\[IndentingNewLine]", 
   RowBox[{"Module", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"c", ",", 
       RowBox[{"dest", "=", "destBuf"}], ",", 
       RowBox[{"src", "=", "srcBuf"}]}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"While", "[", 
       RowBox[{
        RowBox[{"Not", "[", 
         RowBox[{"CBemptyQ", "[", "src", "]"}], "]"}], ",", 
        "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{
          RowBox[{"{", 
           RowBox[{"src", ",", "c"}], "}"}], "=", 
          RowBox[{"CBpop", "[", "src", "]"}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"dest", "=", 
          RowBox[{"CBpush", "[", 
           RowBox[{"dest", ",", "c"}], "]"}]}]}]}], "]"}], ";", 
      "\[IndentingNewLine]", 
      RowBox[{"{", 
       RowBox[{"dest", ",", "src"}], "}"}]}]}], "]"}]}], ";"}]}], "Input",
