  3.689106533190733*^9}, {3.68910665678625*^9, 3.6891066649936953`*^9}, {
  3.689106696916838*^9, 3.6891067069299393`*^9}, {3.689107183735426*^9, 
  3.689107185503257*^9}, {3.689254612075272*^9, 3.689254696545581*^9}, {
