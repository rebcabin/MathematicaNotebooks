Cell[TextData[{
 "The action ",
 StyleBox["save",
  FontSlant->"Italic"],
 " with argument ",
 StyleBox["validated",
  FontSlant->"Italic"],
 " increments the metric for validated characters and pushes the character to \
",
 StyleBox["scannerState",
  FontSlant->"Italic"],
 ". ",
 StyleBox["Save",
  FontSlant->"Italic"],
 " with argument ",
 StyleBox["provisional",
  FontSlant->"Italic"],
 " a character into ",
 StyleBox["provisionalPayload",
  FontSlant->"Italic"],
 " whence it can be washed back when a fault is detected."
}], "Text",
 CellChangeTimes->{{3.91858840157082*^9, 3.9185884461322403`*^9}, {
  3.918588497152426*^9, 3.918588558985058*^9}, {3.918588622249806*^9, 
  3.918588622402515*^9}, {3.918593228255542*^9, 3.9185932446160173`*^9}, {
  3.918593698854763*^9, 3.9185937108896*^9}, {3.918593903861162*^9, 
  3.918593912087304*^9}, {3.9186455234035797`*^9, 3.918645528162673*^9}, {
  3.91866021013278*^9, 
  3.918660227413303*^9}},ExpressionUUID->"ff915570-cb80-46c4-a25b-\
5d3b68cbc6af"]
}, Open  ]],

