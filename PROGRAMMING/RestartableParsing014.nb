Cell[TextData[{
 "Notice that ",
 Cell[BoxData[
  FormBox[
   StyleBox["len",
    FontSlant->"Italic"], TraditionalForm]],ExpressionUUID->
  "c3185910-6888-4d46-a652-1f708a403801"],
 " can never be exactly ",
 StyleBox["cap",
  FontSlant->"Italic"],
 ". When ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    StyleBox["len",
     FontSlant->"Italic"], "=", 
    RowBox[{
     StyleBox["cap",
      FontSlant->"Italic"], "-", "1"}]}], TraditionalForm]],ExpressionUUID->
  "451a0184-6bee-4b84-94c5-8a1b3e81f80f"],
