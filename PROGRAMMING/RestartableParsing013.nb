Cell[BoxData[{
 RowBox[{
  RowBox[{"ClearAll", "[", "fsmStep", "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"With", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"(*", "constants", "*)"}], "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"uCode", "=", 
       RowBox[{"First", "@", 
        RowBox[{"ToCharacterCode", "[", "\"\<u\>\"", "]"}]}]}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{"eCode", "=", 
       RowBox[{"First", "@", 
        RowBox[{"ToCharacterCode", "[", "\"\<e\>\"", "]"}]}]}]}], "}"}], ",", 
    "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"fsmStep", "[", 
      RowBox[{
       RowBox[{"{", "\[IndentingNewLine]", 
        RowBox[{
        "oldVertex_", ",", "\[IndentingNewLine]", "ignoredActionParam_", ",", 
         "\[IndentingNewLine]", "ignoredActionParamParam_", ",", 
         "\[IndentingNewLine]", "matchesParam_"}], "}"}], ",", 
       "\[IndentingNewLine]", "character_"}], "]"}], ":=", 
     "\[IndentingNewLine]", 
     RowBox[{"Module", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"(*", 
         RowBox[{"local", " ", "variables"}], "*)"}], "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"matches", "=", "matchesParam"}], ",", "\[IndentingNewLine]",
          "newVertex", ",", "\[IndentingNewLine]", "action", ",", 
         "\[IndentingNewLine]", "actionArgument"}], "}"}], ",", 
       "\[IndentingNewLine]", "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{
         RowBox[{"{", 
          RowBox[{"newVertex", ",", "action", ",", "actionArgument"}], "}"}], 
         "=", "\[IndentingNewLine]", "\[IndentingNewLine]", 
         RowBox[{"Switch", "[", 
          RowBox[{
          "oldVertex", ",", "\[IndentingNewLine]", "\[IndentingNewLine]", 
           "\"\<Entry\>\"", ",", "\[IndentingNewLine]", 
           RowBox[{"{", 
            RowBox[{
            "\"\<A\>\"", ",", "\"\<no action\>\"", ",", "\"\<no arg\>\""}], 
            "}"}], ",", "\[IndentingNewLine]", "\[IndentingNewLine]", 
           "\"\<A\>\"", ",", "\[IndentingNewLine]", 
           RowBox[{"Switch", "[", 
            RowBox[{"character", ",", "\[IndentingNewLine]", "uCode", ",", 
             RowBox[{"{", 
              RowBox[{
              "\"\<B\>\"", ",", "\"\<save\>\"", ",", "\"\<validated\>\""}], 
              "}"}], ",", "\[IndentingNewLine]", "default_", ",", 
             RowBox[{"{", 
              RowBox[{
              "\"\<A\>\"", ",", "\"\<discard\>\"", ",", "\"\<no arg\>\""}], 
              "}"}]}], "]"}], ",", "\[IndentingNewLine]", 
           "\[IndentingNewLine]", "\"\<B\>\"", ",", "\[IndentingNewLine]", 
           RowBox[{"Switch", "[", 
            RowBox[{"character", ",", "\[IndentingNewLine]", "uCode", ",", 
             RowBox[{"{", 
              RowBox[{
              "\"\<B\>\"", ",", "\"\<discard\>\"", ",", "\"\<no arg\>\""}], 
              "}"}], ",", "\[IndentingNewLine]", "eCode", ",", 
             RowBox[{"{", 
              RowBox[{
              "\"\<C\>\"", ",", "\"\<save\>\"", ",", "\"\<validated\>\""}], 
              "}"}], ",", "\[IndentingNewLine]", "default_", ",", 
             RowBox[{"{", 
              RowBox[{
              "\"\<A\>\"", ",", "\"\<clear\>\"", ",", "\"\<no arg\>\""}], 
              "}"}]}], "]"}], ",", "\[IndentingNewLine]", 
           "\[IndentingNewLine]", "\"\<C\>\"", ",", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{
              RowBox[{"matches", "[", "\"\<T\>\"", "]"}], "=", "character"}], 
             ";", 
             RowBox[{"{", 
              RowBox[{
              "\"\<D\>\"", ",", "\"\<save\>\"", ",", "\"\<validated\>\""}], 
              "}"}]}], ")"}], ",", "\[IndentingNewLine]", 
           "\[IndentingNewLine]", "\"\<D\>\"", ",", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{
              RowBox[{"matches", "[", "\"\<L\>\"", "]"}], "=", "character"}], 
             ";", 
             RowBox[{"{", 
              RowBox[{
              "\"\<Exit\>\"", ",", "\"\<save\>\"", ",", "\"\<validated\>\""}],
               "}"}]}], ")"}], ",", "\[IndentingNewLine]", 
           "\[IndentingNewLine]", "\"\<Exit\>\"", ",", "\[IndentingNewLine]", 
           RowBox[{"{", 
            RowBox[{
            "\"\<Exit\>\"", ",", "\"\<no action\>\"", ",", "\"\<no arg\>\""}],
             "}"}], ",", "\[IndentingNewLine]", "\[IndentingNewLine]", 
           "default_", ",", "\[IndentingNewLine]", 
           RowBox[{"{", 
            RowBox[{"0", ",", 
             RowBox[{"Throw", "[", 
              RowBox[{"\"\<Illegal fsm vertex: \>\"", "<>", 
               RowBox[{"ToString", "@", "oldVertex"}]}], "]"}]}], "}"}]}], 
          "\[IndentingNewLine]", "]"}]}], ";", "\[IndentingNewLine]", 
        "\[IndentingNewLine]", 
        RowBox[{"(*", 
         RowBox[{
          RowBox[{"Print", "[", 
           RowBox[{"<|", "\[IndentingNewLine]", 
            RowBox[{
             RowBox[{"\"\<oldVertex\>\"", "\[Rule]", "oldVertex"}], ",", 
             "\[IndentingNewLine]", 
             RowBox[{"\"\<action\>\"", "\[Rule]", "action"}], ",", 
             "\[IndentingNewLine]", 
             RowBox[{"\"\<arg\>\"", "\[Rule]", "actionArgument"}], ",", 
             "\[IndentingNewLine]", 
             RowBox[{"\"\<newVertex\>\"", "\[Rule]", "newVertex"}], ",", 
             "\[IndentingNewLine]", 
             RowBox[{"\"\<matches\>\"", "\[Rule]", "matchesParam"}], ",", 
             "\[IndentingNewLine]", 
             RowBox[{"\"\<new matches\>\"", "\[Rule]", "matches"}]}], "|>"}], 
           "]"}], ";"}], "*)"}], "\[IndentingNewLine]", "\[IndentingNewLine]", 
        RowBox[{"(*", "return", "*)"}], "\[IndentingNewLine]", 
        RowBox[{"{", 
         RowBox[{
         "newVertex", ",", "action", ",", "actionArgument", ",", "matches"}], 
         "}"}]}]}], "\[IndentingNewLine]", "\[IndentingNewLine]", "]"}]}]}], 
   "]"}], ";"}]}], "Input",
 CellChangeTimes->{{3.689174654874157*^9, 3.689174663536401*^9}, {
