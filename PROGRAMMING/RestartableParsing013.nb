Cell[TextData[{
 "In ",
 StyleBox["Entry",
  FontSlant->"Italic"],
 ", initialize variables, collect a character, and transition to vertex ",
 Cell[BoxData[
  FormBox["A", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "cd36789a-2862-4526-8cb0-b4db631b4f15"],
 " with that character and no action. "
}], "Text",
 CellChangeTimes->{{3.9185676400198393`*^9, 3.918567649557736*^9}, {
   3.9185677354705963`*^9, 3.918567767205346*^9}, {3.918567927897341*^9, 
