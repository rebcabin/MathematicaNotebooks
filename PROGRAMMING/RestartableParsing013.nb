Cell[TextData[{
 "The scanner and FSM maintain a dictionary called ",
 StyleBox["matches",
  FontSlant->"Italic"],
 " for edges labeled with meta-variables ",
 Cell[BoxData[
  FormBox["L", TraditionalForm]],ExpressionUUID->
  "f58adabf-4d21-4cab-b36c-e8775e24c014"],
 " and ",
 Cell[BoxData[
  FormBox["T", TraditionalForm]],ExpressionUUID->
  "a81621d1-a0a4-476b-90c7-98428607e041"],
 ". This dictionary lets us look up the values of ",
 Cell[BoxData[
  FormBox["L", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "999c6261-0b27-4727-ab4f-6e228de97b77"],
 " and ",
 Cell[BoxData[
  FormBox["T", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "f216eb55-2d60-4fb7-84cf-c6c146158591"],
 " by name. "
}], "Text",
