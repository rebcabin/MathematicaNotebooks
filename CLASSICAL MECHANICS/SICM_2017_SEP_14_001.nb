     RowBox[{"fvar", "=", 
      RowBox[{"Unique", "[", "]"}]}], "}"}], ",", 
    RowBox[{"(*", " ", 
     RowBox[{
     "guaranteed", " ", "not", " ", "to", " ", "collide", " ", "with", " ", 
