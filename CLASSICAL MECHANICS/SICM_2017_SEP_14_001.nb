Cell[CellGroupData[{

Cell["(p.21)\tVaried Free-Particle Action", "Section",
 CellChangeTimes->{{3.398348204656227*^9, 3.3983482144002385`*^9}, {
  3.4005198580251446`*^9, 3.400519860593402*^9}, {3.4015849297269726`*^9, 
  3.401584930087491*^9}},ExpressionUUID->"80309c68-8acb-409b-9aa9-\
d2bcf7404675"],

Cell[TextData[{
 "A varied free-particle action takes a mass, a coordinate path, a candidate \
variation function, and two endpoints and returns a function of a single real \
parameter, \[Epsilon], which is a scale factor applied to the clamped \
variation function. SICM would write the new, varied path as ",
 Cell[BoxData[
  FormBox[
   RowBox[{"q", "+", 
    RowBox[{"\[Epsilon]", " ", "\[Eta]"}]}], TraditionalForm]],
  ExpressionUUID->"e809fe08-656d-41e9-88fd-bf8b8323b4d9"],
 ", but we must write it as ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"\[Lambda]", " ", 
     RowBox[{"t", ".", " ", 
      RowBox[{"q", "(", "t", ")"}]}]}], " ", "+", " ", 
    RowBox[{"\[Epsilon]", " ", 
     RowBox[{"\[Eta]", "(", "t", ")"}]}]}], TraditionalForm]],ExpressionUUID->
  "2a09acc3-6f78-4270-84fd-c176bf585bb3"],
 " because we ",
 "cannot",
 " add functions the way we add function values:"
}], "Text",
 CellChangeTimes->{{3.400356542993416*^9, 3.4003567285619707`*^9}, {
   3.400509021313582*^9, 3.4005090224887*^9}, 3.4005800149391212`*^9, {
   3.4008584128880463`*^9, 
   3.400858414698227*^9}},ExpressionUUID->"004dec9b-3015-4ff6-9765-\
d357b732db82"],

Cell[BoxData[
 RowBox[{
  RowBox[{"variedFreeParticleAction", "[", 
   RowBox[{"m_", ",", "q_", ",", "\[Nu]_", ",", "t1_", ",", "t2_"}], "]"}], ":=",
   "\[IndentingNewLine]", 
  RowBox[{"Function", "[", 
   RowBox[{"\[Epsilon]", ",", "\[IndentingNewLine]", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"\[Eta]", "=", 
        RowBox[{"make\[Eta]", "[", 
         RowBox[{"\[Nu]", ",", "t1", ",", "t2"}], "]"}]}], "}"}], ",", 
      "\[IndentingNewLine]", 
      RowBox[{"lagrangianAction", "[", 
       RowBox[{
        RowBox[{"lFreeParticle", "[", "m", "]"}], ",", "\[IndentingNewLine]", 
        RowBox[{"Function", "[", 
         RowBox[{"t", ",", 
          RowBox[{
           RowBox[{"q", "[", "t", "]"}], "+", 
           RowBox[{"\[Epsilon]", " ", 
            RowBox[{"\[Eta]", "[", "t", "]"}]}]}]}], "]"}], ",", "t1", ",", 
        "t2"}], "]"}]}], "]"}]}], "]"}], " ", 
  RowBox[{"(*", " ", 
   FormBox[
    RowBox[{
     RowBox[{"\[Lambda]", " ", 
      RowBox[{"t", ".", " ", 
       RowBox[{"q", "(", "t", ")"}]}]}], " ", "+", " ", 
     RowBox[{"\[Epsilon]", " ", 
      RowBox[{"\[Eta]", "(", "t", ")"}]}]}],
    TraditionalForm], " ", "*)"}]}]], "Input",
 CellChangeTimes->{{3.3975886204401407`*^9, 3.3975886673138275`*^9}, {
  3.400356748102925*^9, 3.4003567840715213`*^9}, {3.4005090445749083`*^9, 
  3.400509049060356*^9}},
 CellLabel->
  "In[213]:=",ExpressionUUID->"1a581e2e-6123-4e5c-a419-e4b4c60887a7"],

