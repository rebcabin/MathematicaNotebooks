Cell["\<\
How about a packaged-up version, now, that plots the intermediate polynomials \
for any order?\
\>", "Text",
 CellChangeTimes->{{3.4005349918145604`*^9, 3.400535021225501*^9}, {
  3.4005364273420987`*^9, 
  3.400536432621627*^9}},ExpressionUUID->"1f8e077d-6606-47de-a127-\
269b114c7eec"],

Cell[BoxData[
 RowBox[{
  RowBox[{"plottingFindPath", "[", 
   RowBox[{
   "lagrangian_", ",", " ", "t0_", ",", " ", "q0_", ",", " ", "t1_", ",", " ",
     "q1_", ",", " ", "n_"}], "]"}], " ", ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"happyJoy", " ", "=", " ", 
       RowBox[{"Reap", "[", 
        RowBox[{"findPath", "[", 
         RowBox[{
         "lagrangian", ",", " ", "t0", ",", " ", "q0", ",", " ", "t1", ",", 
          " ", "q1", ",", " ", "n"}], "]"}], "]"}]}], ",", "\n", "   ", 
      "joys1", ",", " ", "joys2", ",", " ", "tees2", ",", " ", "joys25", ",", 
      " ", "joys3"}], "}"}], ",", "\n", "  ", 
    RowBox[{
     RowBox[{"joys1", " ", "=", " ", 
      RowBox[{"happyJoy", "\[LeftDoubleBracket]", 
       RowBox[{"2", ",", " ", "1"}], "\[RightDoubleBracket]"}]}], ";", "\n", 
     "  ", 
     RowBox[{"joys2", " ", "=", " ", 
      RowBox[{"Map", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"Join", "[", 
          RowBox[{"q0", ",", " ", "#", ",", " ", "q1"}], "]"}], " ", "&"}], 
        ",", " ", "joys1"}], "]"}]}], ";", "\n", "  ", 
     RowBox[{"tees2", " ", "=", " ", 
      RowBox[{"bookendedInterpolantsR1", "[", 
       RowBox[{"t0", ",", " ", "t1", ",", " ", "n"}], "]"}]}], ";", "\n", 
     "  ", 
     RowBox[{"joys25", " ", "=", " ", 
      RowBox[{"Map", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"Thread", "[", 
          RowBox[{"{", 
           RowBox[{"tees2", ",", " ", "#"}], "}"}], "]"}], " ", "&"}], ",", 
        " ", "joys2"}], "]"}]}], ";", "\n", "  ", 
     RowBox[{"joys3", " ", "=", " ", 
      RowBox[{"Expand", "@", 
       RowBox[{"Map", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"InterpolatingPolynomial", "[", 
           RowBox[{"#", ",", " ", "t"}], "]"}], " ", "&"}], ",", " ", 
         "joys25"}], "]"}]}]}], ";", "\n", "  ", 
     RowBox[{"Plot", "[", 
      RowBox[{"joys3", ",", " ", 
       RowBox[{"{", 
        RowBox[{"t", ",", " ", "t0", ",", " ", "t1"}], "}"}]}], "]"}]}]}], 
   "]"}]}]], "Input",
 CellChangeTimes->{{3.4005362738597517`*^9, 3.4005363860569706`*^9}, {
  3.4009589710892854`*^9, 3.4009589736815443`*^9}, {3.4009591948516593`*^9, 
  3.4009592022033944`*^9}},
 CellLabel->
  "In[292]:=",ExpressionUUID->"e1928943-c6fc-4226-a577-6b9938caafb2"],

Cell["\<\
Scrunch it up, just for fun. As an aside, the above corresponds to Haskell \
\"do\" notation for monadic sequences, the below corresponds to an equivalent \
explicit nesting of function calls. Because of Plot' s peculiar evaluation \
rules, we must have one variable defined, otherwise colors will not display \
properly. This computation is not really monadic because it is not purely \
functional.\
\>", "Text",
 CellChangeTimes->{{3.40059843774179*^9, 3.4005985629543095`*^9}, {
  3.40059898450146*^9, 3.400599028125822*^9}, {3.4005992359766054`*^9, 
  3.400599238991907*^9}},ExpressionUUID->"302b36cb-a93f-4e68-958d-\
21279cf7fc50"],

Cell[BoxData[
 RowBox[{
  RowBox[{"plottingFindPath", "[", 
   RowBox[{
   "lagrangian_", ",", " ", "t0_", ",", " ", "q0_", ",", " ", "t1_", ",", " ",
     "q1_", ",", " ", "n_"}], "]"}], " ", ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"j", " ", "=", " ", 
      RowBox[{"Expand", "@", 
       RowBox[{"Map", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"InterpolatingPolynomial", "[", 
           RowBox[{"#", ",", " ", "t"}], "]"}], " ", "&"}], ",", " ", 
         RowBox[{"Map", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"Thread", "[", 
             RowBox[{"{", 
              RowBox[{
               RowBox[{"bookendedInterpolantsR1", "[", 
                RowBox[{"t0", ",", " ", "t1", ",", " ", "n"}], "]"}], ",", 
               " ", "#"}], "}"}], "]"}], " ", "&"}], ",", " ", 
           RowBox[{"Map", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"Join", "[", 
               RowBox[{"q0", ",", " ", "#", ",", " ", "q1"}], "]"}], " ", 
              "&"}], ",", " ", 
             RowBox[{
              RowBox[{"Reap", "[", 
               RowBox[{"findPath", "[", 
                RowBox[{
                "lagrangian", ",", " ", "t0", ",", " ", "q0", ",", " ", "t1", 
                 ",", " ", "q1", ",", " ", "n"}], "]"}], "]"}], 
              "\[LeftDoubleBracket]", 
              RowBox[{"2", ",", " ", "1"}], "\[RightDoubleBracket]"}]}], 
            "]"}]}], "]"}]}], "]"}]}]}], "}"}], ",", "\n", "  ", 
    RowBox[{"Plot", "[", 
     RowBox[{"j", ",", " ", 
      RowBox[{"{", 
       RowBox[{"t", ",", " ", "t0", ",", " ", "t1"}], "}"}]}], "]"}]}], 
   "]"}]}]], "Input",
