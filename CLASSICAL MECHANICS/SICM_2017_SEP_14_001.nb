Cell[TextData[{
 "Invent an arbitrary ",
 StyleBox["variation function",
  FontWeight->"Bold"],
 " into ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["\[DoubleStruckCapitalR]", "3"], TraditionalForm]],
  ExpressionUUID->"98e218d2-ee43-4b4a-9eb9-030bf15a2d86"],
 " that we can add to the testPath:"
}], "Text",
 CellChangeTimes->{{3.39808575135769*^9, 3.3980857925328074`*^9}, {
  3.398445428450906*^9, 3.398445451028163*^9}, {3.4005088240188546`*^9, 
  3.400508826801133*^9}, {3.400858230549814*^9, 
  3.400858232276987*^9}},ExpressionUUID->"2faebf69-cbd9-4a7a-9d8a-\
acd9686342ec"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"testVariation", "[", "t_", "]"}], ":=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"Sin", "[", "t", "]"}], ",", 
     RowBox[{"8", 
      RowBox[{"Cos", "[", 
       RowBox[{"8", "t"}], "]"}]}], ",", 
     SuperscriptBox["t", "2"]}], "}"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.397589798992984*^9, 3.397589813791464*^9}, {
   3.397590452099288*^9, 3.3975904843315115`*^9}, {3.398109257376235*^9, 
   3.3981092574632435`*^9}, 3.3981093326517615`*^9, 3.3981093645089474`*^9},
 CellLabel->
  "In[205]:=",ExpressionUUID->"4c3809ce-67c7-41ba-a174-5165daf902af"],

Cell[TextData[{
 "Force this and ",
 StyleBox["all other path variations",
  FontSlant->"Italic"],
 " to vanish at the endpoints of an action integral. Clamp any variation by \
multiplying by a scale-free quadratic. This is better than the SICM original \
because the clamping factor, ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"4", 
     RowBox[{"(", 
      RowBox[{"t", "-", "t1"}], ")"}], 
     RowBox[{"(", 
      RowBox[{"t2", "-", "t"}], ")"}]}], 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{"t2", "-", "t1"}], ")"}], "2"]], TraditionalForm]],
  ExpressionUUID->"3149cf37-2161-46a9-8300-6fe89d04bf35"],
 ", varies from 0 to 1 no matter what ",
 Cell[BoxData[
  FormBox[
   StyleBox["t1",
    FontSlant->"Italic"], TraditionalForm]],ExpressionUUID->
  "580bc287-f287-422f-96d4-9f0b257c2f1d"],
 " and ",
 Cell[BoxData[
  FormBox[
   StyleBox["t2",
    FontSlant->"Italic"], TraditionalForm]],ExpressionUUID->
  "c5418595-6ede-4e65-8c8c-b7962781d5fb"],
 " might be, so long as ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    StyleBox["t1",
     FontSlant->"Italic"], "<", 
    StyleBox["t2",
     FontSlant->"Italic"]}], TraditionalForm]],ExpressionUUID->
  "553c0a26-05cb-4ad6-a61d-52718288e301"],
 ". "
}], "Text",
 CellChangeTimes->{{3.397588411443243*^9, 3.397588436276726*^9}, {
   3.397590155180599*^9, 3.3975901699240737`*^9}, {3.398084987681999*^9, 
   3.3980850507133904`*^9}, {3.3980851588297625`*^9, 
   3.3980851708033676`*^9}, {3.3980854659841557`*^9, 
   3.3980855971242685`*^9}, {3.3981092658130784`*^9, 3.398109287223219*^9}, 
   3.4003563316712856`*^9, {3.4005088363050833`*^9, 3.400508880566509*^9}, {
   3.4008582617859373`*^9, 
   3.4008582713768964`*^9}},ExpressionUUID->"3c539e8e-f241-4a82-bc00-\
c77b053ad11c"],

Cell[BoxData[
 RowBox[{
  RowBox[{"clampingFactor", "[", 
   RowBox[{"t_", ",", "t1_", ",", "t2_"}], "]"}], ":=", 
  FractionBox[
   RowBox[{"4", 
    RowBox[{"(", 
     RowBox[{"t", "-", "t1"}], ")"}], 
    RowBox[{"(", 
     RowBox[{"t2", "-", "t"}], ")"}]}], 
   SuperscriptBox[
    RowBox[{"(", 
     RowBox[{"t2", "-", "t1"}], ")"}], "2"]]}]], "Input",
 CellChangeTimes->{{3.4003564002371416`*^9, 3.4003564323213496`*^9}},
 CellLabel->
  "In[206]:=",ExpressionUUID->"5eef2847-9dd6-43bb-b587-0d730b39b8ee"],

Cell[TextData[{
 "Every time you evaluate the test expression below, you will get the same \
y-values and the same shape of curve, but different ",
 StyleBox["t1",
  FontSlant->"Italic"],
 " and ",
 StyleBox["t2",
  FontSlant->"Italic"],
 ":"
}], "Text",
