Cell[TextData[{
 "In linear algebra, vectors are columns, i.e., ",
 Cell[BoxData[
  FormBox[
   RowBox[{"n", "\[Times]", "1"}], TraditionalForm]],ExpressionUUID->
  "a096b99b-cac4-40be-be0d-f5aea1e3876f"],
 " matrices, and co-vectors are rows, i.e., ",
 Cell[BoxData[
  FormBox[
   RowBox[{"1", "\[Times]", "n"}], TraditionalForm]],ExpressionUUID->
  "12d9ef63-7a3d-4949-9ed2-e83c8a247183"],
 " matrices (see ",
 ButtonBox["Vector Calculus, Linear Algebra, and Differential Forms, A \
Unified Approach",
  BaseStyle->"Hyperlink",
  ButtonData->{
    URL["https://www.amazon.com/Calculus-Algebra-Differential-Unified-\
Approach/dp/0971576653/"], None},
  ButtonNote->
   "https://www.amazon.com/Calculus-Algebra-Differential-Unified-Approach/dp/\
0971576653/"],
 " by John H. Hubbard and Barbara Burke Hubbard)."
}], "Text",
 CellChangeTimes->{{3.729171298029805*^9, 3.729171328988427*^9}, {
   3.729171450083456*^9, 3.729171568542794*^9}, {3.7618233440035915`*^9, 
   3.761823346114144*^9}, {3.764548006209711*^9, 3.764548054072945*^9}, 
