Cell[TextData[{
 "We begin with MLE on a familiar school example: estimate the slope ",
 Cell[BoxData[
  FormBox["m", TraditionalForm]],ExpressionUUID->
  "3c5c8762-e2fa-4f86-a720-cfc8afe368c5"],
 " and intercept ",
 Cell[BoxData[
  FormBox["b", TraditionalForm]],ExpressionUUID->
  "5fb46476-6428-4331-bb2c-4a3b7b7083cc"],
 " of the best-fit line function ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{"z", "(", "x", ")"}], "=", 
    RowBox[{
     RowBox[{"m", " ", "x"}], "+", "b"}]}], TraditionalForm]],ExpressionUUID->
  "ad03b80a-54ed-4608-a129-041b31a04416"],
 " to noisy data ",
 Cell[BoxData[
  FormBox[
   RowBox[{"(", 
    RowBox[{
     SubscriptBox["x", "i"], ",", 
     SubscriptBox["z", "i"]}], ")"}], TraditionalForm]],ExpressionUUID->
  "8d32931c-b04a-48ce-894a-7824bda280f4"],
 ". "
}], "Text",
