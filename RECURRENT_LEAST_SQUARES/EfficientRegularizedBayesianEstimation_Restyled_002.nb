  FormBox[
   RowBox[{
    SubscriptBox["P", "\[CapitalZeta]"], "=", 
    RowBox[{
     SuperscriptBox[
      SubscriptBox["\[Sigma]", "\[Zeta]"], "2"], "\[ScriptCapitalI]"}]}], 
   TraditionalForm]],ExpressionUUID->"049f9b03-fb66-4125-b445-13e5236de60f"],
