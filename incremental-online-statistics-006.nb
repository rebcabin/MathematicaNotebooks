Cell[BoxData[
 FormBox[
  RowBox[{
   SubscriptBox[
    OverscriptBox["z", "_"], 
    RowBox[{"n", "+", "1"}]], "=", 
   RowBox[{
    RowBox[{
     SubscriptBox[
      OverscriptBox["z", "_"], "n"], "+", 
     RowBox[{"K", "\[Times]", 
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["z", 
