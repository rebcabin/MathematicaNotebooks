      RowBox[{"i", "=", "1"}], "n"], 
     SuperscriptBox[
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["z", "i"], "-", 
        SubscriptBox[
         OverscriptBox["z", "_"], "n"]}], ")"}], "2"]}]}], TraditionalForm]],
  ExpressionUUID->"d07e0069-f6c4-4614-964e-3711b8a05fd7"],
