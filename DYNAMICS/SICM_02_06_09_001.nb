Cell[TextData[{
 "Invent an arbitrary ",
 StyleBox["variation function",
  FontWeight->"Bold"],
 " into ",
 Cell[BoxData[
  FormBox[
   SuperscriptBox["\[DoubleStruckCapitalR]", "3"], TraditionalForm]],
  ExpressionUUID->"936e81ab-1755-4961-9805-dd0d025e4929"],
 " that we can add to the testPath:"
}], "Text",
 CellChangeTimes->{{3.39808575135769*^9, 3.3980857925328074`*^9}, {
  3.398445428450906*^9, 3.398445451028163*^9}, {3.4005088240188546`*^9, 
  3.400508826801133*^9}, {3.400858230549814*^9, 
  3.400858232276987*^9}},ExpressionUUID->"a946bf16-8e75-47b1-b5f2-\
8230ce69e761"],

Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"testVariation", "[", "t_", "]"}], ":=", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"Sin", "[", "t", "]"}], ",", 
     RowBox[{"8", 
      RowBox[{"Cos", "[", 
       RowBox[{"8", "t"}], "]"}]}], ",", 
     SuperscriptBox["t", "2"]}], "}"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.397589798992984*^9, 3.397589813791464*^9}, {
   3.397590452099288*^9, 3.3975904843315115`*^9}, {3.398109257376235*^9, 
   3.3981092574632435`*^9}, 3.3981093326517615`*^9, 
   3.3981093645089474`*^9},ExpressionUUID->"aa61f179-c695-46bc-b941-\
413fad1b213a"],

Cell[TextData[{
 "Force this and ",
 StyleBox["all other path variations",
  FontSlant->"Italic"],
 " to vanish at the endpoints of an action integral. Clamp any variation by \
multiplying by a scale-free quadratic. This is better than the SICM original \
because the clamping factor, ",
 Cell[BoxData[
  FormBox[
   FractionBox[
    RowBox[{"4", 
     RowBox[{"(", 
      RowBox[{"t", "-", "t1"}], ")"}], 
     RowBox[{"(", 
      RowBox[{"t2", "-", "t"}], ")"}]}], 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{"t2", "-", "t1"}], ")"}], "2"]], TraditionalForm]],
  ExpressionUUID->"21d0f265-8f34-4639-bcbf-9c7954399018"],
 ", varies from 0 to 1 no matter what ",
 Cell[BoxData[
  FormBox[
   StyleBox["t1",
    FontSlant->"Italic"], TraditionalForm]],ExpressionUUID->
  "c1bbd2ac-3c89-417d-939d-a925da92b97b"],
 " and ",
 Cell[BoxData[
  FormBox[
   StyleBox["t2",
    FontSlant->"Italic"], TraditionalForm]],ExpressionUUID->
  "c9b95f6b-fe29-4540-a72c-1fa5dd02ed1a"],
 " might be, so long as ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    StyleBox["t1",
     FontSlant->"Italic"], "<", 
    StyleBox["t2",
     FontSlant->"Italic"]}], TraditionalForm]],ExpressionUUID->
  "a77e14d6-f0b3-4f32-8ead-4ef2f43bd2e9"],
 ". "
}], "Text",
 CellChangeTimes->{{3.397588411443243*^9, 3.397588436276726*^9}, {
   3.397590155180599*^9, 3.3975901699240737`*^9}, {3.398084987681999*^9, 
   3.3980850507133904`*^9}, {3.3980851588297625`*^9, 
   3.3980851708033676`*^9}, {3.3980854659841557`*^9, 
   3.3980855971242685`*^9}, {3.3981092658130784`*^9, 3.398109287223219*^9}, 
   3.4003563316712856`*^9, {3.4005088363050833`*^9, 3.400508880566509*^9}, {
   3.4008582617859373`*^9, 
   3.4008582713768964`*^9}},ExpressionUUID->"67b853cc-cba2-4f2b-90a7-\
ac75d0e65e21"],

Cell[BoxData[
 RowBox[{
  RowBox[{"clampingFactor", "[", 
   RowBox[{"t_", ",", "t1_", ",", "t2_"}], "]"}], ":=", 
  FractionBox[
   RowBox[{"4", 
    RowBox[{"(", 
     RowBox[{"t", "-", "t1"}], ")"}], 
    RowBox[{"(", 
     RowBox[{"t2", "-", "t"}], ")"}]}], 
   SuperscriptBox[
    RowBox[{"(", 
     RowBox[{"t2", "-", "t1"}], ")"}], "2"]]}]], "Input",
 CellChangeTimes->{{3.4003564002371416`*^9, 
  3.4003564323213496`*^9}},ExpressionUUID->"0868edab-a004-4a81-aa92-\
45145d9291d7"],

