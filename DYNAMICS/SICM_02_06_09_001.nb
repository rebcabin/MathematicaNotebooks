Cell[TextData[Cell[BoxData[
 FormBox[
  RowBox[{
   RowBox[{
    RowBox[{"\[ScriptCapitalS]", "[", "\[Gamma]", "]"}], 
    RowBox[{"(", 
     RowBox[{
      SubscriptBox["t", "1"], ",", 
      SubscriptBox["t", "2"]}], ")"}]}], " ", "=", " ", 
   RowBox[{
    RowBox[{
     SubsuperscriptBox["\[Integral]", 
      SubscriptBox["t", "1"], 
      SubscriptBox["t", "2"]], " ", 
     RowBox[{"\[ScriptCapitalL]", "\[SmallCircle]", " ", 
      RowBox[{"\[ScriptCapitalT]", "[", "\[Gamma]", "]"}]}]}], "=", " ", 
    RowBox[{
     SubsuperscriptBox["\[Integral]", 
      SubscriptBox["t", "1"], 
      SubscriptBox["t", "2"]], " ", 
     RowBox[{
      SubscriptBox["L", "\[Chi]"], "\[SmallCircle]", " ", 
      RowBox[{"\[CapitalGamma]", "[", "q", "]"}]}]}]}]}], 
  TraditionalForm]],ExpressionUUID->"e8f70093-6cc3-40d3-b142-06b14dc265a0"]], \
"Text",
