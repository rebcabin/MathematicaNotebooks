Cell[BoxData[
 RowBox[{
  RowBox[{"functionMaker", "[", "tuples_List", "]"}], ":=", 
  "\[IndentingNewLine]", 
  RowBox[{"Function", "[", 
   RowBox[{"a", ",", "\[IndentingNewLine]", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"tuple", "=", 
        RowBox[{"Select", "[", 
         RowBox[{"tuples", ",", 
          RowBox[{
           RowBox[{"a", "===", 
            RowBox[{
            "#", "\[LeftDoubleBracket]", "1", "\[RightDoubleBracket]"}]}], 
           "&"}]}], "]"}]}], "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{"If", "[", 
