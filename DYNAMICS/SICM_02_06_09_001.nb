  "d2d2bf1b-63da-4723-80bd-b0134ca25b9e"],
 ". Then ",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["L", "\[Chi]"], "\[SmallCircle]", " ", 
     RowBox[{"\[CapitalGamma]", "[", "q", "]"}]}], "=", 
    RowBox[{
     RowBox[{
      RowBox[{"(", 
       RowBox[{"\[ScriptCapitalL]", "\[SmallCircle]", " ", 
        SubsuperscriptBox["#", "\[Chi]", 
         RowBox[{"-", "1"}]]}], ")"}], "\[SmallCircle]", " ", 
      RowBox[{"(", 
       RowBox[{
        SubscriptBox["#", "\[Chi]"], "\[SmallCircle]", "  ", 
        RowBox[{"\[ScriptCapitalT]", "[", "\[Gamma]", "]"}]}], ")"}]}], "=", 
     RowBox[{
      RowBox[{"(", 
       RowBox[{"\[ScriptCapitalL]", "\[SmallCircle]", " ", 
        RowBox[{"(", 
         RowBox[{
          SubsuperscriptBox["#", "\[Chi]", 
           RowBox[{"-", "1"}]], "\[SmallCircle]", " ", 
          SubscriptBox["#", "\[Chi]"]}], ")"}], "\[SmallCircle]", "  ", 
        RowBox[{"\[ScriptCapitalT]", "[", "\[Gamma]", "]"}]}], ")"}], "=", 
      RowBox[{"\[ScriptCapitalL]", "\[SmallCircle]", "  ", 
       RowBox[{"\[ScriptCapitalT]", "[", "\[Gamma]", "]"}]}]}]}]}], 
   TraditionalForm]],ExpressionUUID->"82b1b5f9-9780-4705-ac96-08d502a39364"],
 " and "
}], "Text",
