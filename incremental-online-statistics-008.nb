Cell[TextData[{
 StyleBox["Fold", "Input"],
 " first computes ",
 Cell[BoxData[
  FormBox[
   RowBox[{"f", "[", 
    RowBox[{"z", ",", "a"}], "]"}], TraditionalForm]],ExpressionUUID->
