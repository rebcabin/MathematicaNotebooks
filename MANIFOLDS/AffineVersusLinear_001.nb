  FormBox[
   SuperscriptBox["\[DoubleStruckCapitalR]", "3"], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "07a3b59e-5130-4f05-a47a-b041e8102853"],
