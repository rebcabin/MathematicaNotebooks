  FormBox[
   RowBox[{"(", 
    RowBox[{"a", ",", "b", ",", "e"}], ")"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "7c72f094-f8f6-401a-bd2d-3a9f251226f9"],
